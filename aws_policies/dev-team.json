{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Sid": "DevS3Access",
      "Effect": "Allow",
      "Action": ["s3:PutObject", "s3:GetObject", "s3:ListBucket"],
      "Resource": ["arn:aws:s3:::dev-assets/*"]
    },
    {
      "Sid": "DevRDSAccess",
      "Effect": "Allow",
      "Action": ["rds:DescribeDBInstances", "rds:RebootDBInstance"],
      "Resource": "arn:aws:rds:*:*:db:dev-*"
    }
  ]
}
